<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="_assets_utils" inherit_id="web._assets_utils">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/theme_treehouse/static/src/scss/mixins.scss"/>
        </xpath>
    </template>
    <template id="_assets_primary_variables" inherit_id="website._assets_primary_variables" priority="1">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/theme_treehouse/static/src/scss/primary_variables.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_treehouse/static/src/scss/snippets_variables.scss"/>
        </xpath>
    </template>
    <template id="_assets_frontend_helpers" inherit_id="website._assets_frontend_helpers" priority="1">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/theme_treehouse/static/src/scss/bootstrap_overridden.scss"/>
        </xpath>
    </template>

    <template id="assets_frontend" inherit_id="website.assets_frontend" priority="1">
        <xpath expr="//link[last()]" position="after">
            <!-- Common -->
            <link rel="stylesheet" type="text/scss" href="/theme_common/static/src/scss/s_team_profiles.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_common/static/src/scss/s_images_carousel.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_common/static/src/scss/s_images_row.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_common/static/src/scss/s_badge.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_common/static/src/scss/s_masonry_block.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_common/static/src/scss/s_media_list.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_common/static/src/scss/s_news_carousel.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_common/static/src/scss/s_process_steps.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_common/static/src/scss/s_progress_bar.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_common/static/src/scss/s_blockquote.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_common/static/src/scss/s_timeline.scss"/>
            <!-- Theme -->
            <link rel="stylesheet" type="text/scss" href="/theme_treehouse/static/src/scss/theme.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_treehouse/static/src/scss/snippets.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_treehouse/static/src/scss/snippets/s_big_message.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_treehouse/static/src/scss/snippets/s_panel_extended.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_treehouse/static/src/scss/snippets/s_share_extended.scss"/>
            <link rel="stylesheet" type="text/scss" href="/theme_treehouse/static/src/scss/snippets/s_well_extended.scss"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/theme_treehouse/static/src/js/theme_animation.js"></script>
        </xpath>
    </template>

    <template id="assets_editor" inherit_id="website.assets_editor">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/theme_common/static/src/js/s_progress_bar.js"></script>
        </xpath>
    </template>
</odoo>
